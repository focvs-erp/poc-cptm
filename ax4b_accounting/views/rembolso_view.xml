<odoo>
    <record id="rembolso_expense_form" model="ir.ui.view">
        <field name="name">Rembolso</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="/form/sheet/group[1]/group[1]/field[@name='payment_reference']" position="after">
                <field name="checkbox_bloqueio"/>
            </xpath>
        </field>
    </record>
</odoo>
